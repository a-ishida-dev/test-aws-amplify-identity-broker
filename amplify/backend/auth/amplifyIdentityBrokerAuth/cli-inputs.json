{
    "version": "1",
    "cognitoConfig": {
        "authSelections": "userPoolOnly",
        "resourceName": "amplifyIdentityBrokerAuth",
        "serviceName": "Cognito",
        "userPoolName": "brokeruserpool",
        "requiredAttributes": [
            "email"
        ],
        "usernameAttributes": [
            "email",
            "phone_number"
        ],
        "userPoolGroups": false,
        "adminQueries": false,
        "mfaConfiguration": "OPTIONAL",
        "mfaTypes": [
            "SMS Text Message",
            "TOTP"
        ],
        "smsAuthenticationMessage": "Your authentication code is {####}",
        "smsVerificationMessage": "Your verification code is {####}",
        "emailVerificationSubject": "Your verification code",
        "emailVerificationMessage": "Your verification code is {####}",
        "passwordPolicyMinLength": 8,
        "passwordPolicyCharacters": [],
        "autoVerifiedAttributes": [
            "email"
        ],
        "userpoolClientGenerateSecret": true,
        "userpoolClientRefreshTokenValidity": 30,
        "userpoolClientWriteAttributes": [
            "email",
            "locale",
            "phone_number",
            "custom:tos_signed",
            "custom:tos_version",
            "custom:newsletter",
            "given_name",
            "family_name",
            "address",
            "birthdate",
            "gender"
        ],
        "userpoolClientReadAttributes": [
            "email",
            "email_verified",
            "locale",
            "phone_number",
            "custom:tos_signed",
            "custom:tos_version",
            "custom:newsletter",
            "given_name",
            "family_name",
            "address",
            "birthdate",
            "gender"
        ],
        "userpoolClientSetAttributes": false,
        "hostedUI": true,
        "hostedUIDomainName": "amplifyidbroker-demo",
        "authProvidersUserPool": [],
        "hostedUIProviderMeta": [],
        "oAuthMetadata": {
            "AllowedOAuthFlows": [
                "code"
            ],
            "AllowedOAuthScopes": [
                "phone",
                "email",
                "openid",
                "profile",
                "aws.cognito.signin.user.admin"
            ],
            "CallbackURLs": [
                "https://injection.placeholder.amazon.com/"
            ],
            "LogoutURLs": [
                "https://injection.placeholder.amazon.com/logout"
            ]
        },
        "triggers": {
            "CustomMessage": [
                "custom"
            ],
            "DefineAuthChallenge": [
                "custom"
            ],
            "CreateAuthChallenge": [
                "custom"
            ],
            "VerifyAuthChallengeResponse": [
                "custom"
            ]
        },
        "dependsOn": [
            {
                "category": "function",
                "resourceName": "amplifyIdentityBrokerCustomMessage",
                "triggerProvider": "Cognito",
                "attributes": [
                    "Arn",
                    "Name"
                ]
            },
            {
                "category": "function",
                "resourceName": "amplifyIdentityBrokerDefineAuthChallenge",
                "triggerProvider": "Cognito",
                "attributes": [
                    "Arn",
                    "Name"
                ]
            },
            {
                "category": "function",
                "resourceName": "amplifyIdentityBrokerCreateAuthChallenge",
                "triggerProvider": "Cognito",
                "attributes": [
                    "Arn",
                    "Name"
                ]
            },
            {
                "category": "function",
                "resourceName": "amplifyIdentityBrokerVerifyAuthChallenge",
                "triggerProvider": "Cognito",
                "attributes": [
                    "Arn",
                    "Name"
                ]
            }
        ]
    }
}